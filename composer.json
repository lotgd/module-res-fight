{
    "name": "lotgd/module-res-fight",
    "description": "A resource module which provides basic fighting capabilities as well as hooks to extend them.",
    "type": "lotgd-module",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Basilius Sauter",
            "email": "basilius.sauter@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LotGD\\Module\\Res\\Fight\\": "src/",
            "LotGD\\Module\\Res\\Fight\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {"type": "path", "url": "../core"},
        {"type": "path", "url": "../module-village"},
        {"type": "path", "url": "../module-new-day"}
    ],

    "require": {
        "lotgd/core": "dev-master as 0.6.0",
        "lotgd/module-village": "^0.6.0",
        "lotgd/module-new-day": "^0.6.0"
    },

    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/yaml": "*"
    },

    "config": {
        "secure-http": false
    }
}
